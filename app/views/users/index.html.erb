<%= render :partial => "profile/myblock", :locals => { :user => current_user } %>

<div class="grid_9 omega" id="profile-right">
  <div class="orangebox">
    <ul>
      <% @users.each do |user| %>
        <li>
          <%= user.login %> | <%= link_to "Show", user %>
        </li>
      <% end %>
    </ul>
    <hr class="clearblock" />
  </div>
</div>
