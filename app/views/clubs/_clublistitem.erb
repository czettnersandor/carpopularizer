<div class="listitem">
  <div class="list-header">
    <%= link_to(club.name, club) %>
  </div>
  <div class="profile-data">
    <%# link_to(image_tag(car.carimages.first.image.url(:thumb)), user_box_path(car.user, car)) if car.carimages.first %>
  </div>
  <div class="description">
    <%= shorten(club.description) %>
  </div>
  <hr class="clear" />
</div>
